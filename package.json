{
  "name": "cryptwist",
  "version": "0.2.0-dev.1",
  "description": "Cryptographic Primitive Implementation for illustration",
  "keywords": [
    "cryptography",
    "encryption",
    "hash",
    "decryption",
    "digest"
  ],
  "author": "Jack Q <QiaoBo@outlook.com> (https://jack-q.github.io/blog/)",
  "main": "lib/index.js",
  "typings": "typings/index.d.ts",
  "license": "MIT",
  "files": [
    "lib",
    "typings"
  ],
  "scripts": {
    "build": "npm run clean && cross-env BABEL_ENV=production babel src -d lib --ignore spec.js,test.js,test-util.js",
    "lint": "eslint --ext .js src",
    "test": "npm run build && npm run lint && jest",
    "coveralls": "npm run build && jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepublishOnly": "npm run test && npm run build",
    "clean": "rimraf -r coverage lib"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^22.1.4",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jack-Q/cryptwist.git"
  },
  "bugs": {
    "url": "https://github.com/Jack-Q/cryptwist/issues"
  },
  "homepage": "https://github.com/Jack-Q/cryptwist#readme"
}
